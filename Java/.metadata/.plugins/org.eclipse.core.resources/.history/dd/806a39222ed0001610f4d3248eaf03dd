package pers.fhr.exceldemo.test;

import pers.fhr.exceldemo.util.ExcelFieldAnnotation;

public class Student {
	@ExcelFieldAnnotation(name="姓名",index=0,width=5)
	private String name;
	@ExcelFieldAnnotation(name="学号",index=1,width=5)
	private int no;
	@ExcelFieldAnnotation(name="余额",index=2,width=5)
	private double count;
	public void setName(String name){
		this.name=name;
	}
	public void setNo(int no){
		this.no=no;
	}
	public void setCount(Double count){
		this.count=count;
	}
	public Student(String name,int no,double count){
		this.name=name;
		this.no=no;
		this.count=count;
	}
}
